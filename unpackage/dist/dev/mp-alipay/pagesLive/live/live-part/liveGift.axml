<block a:if="{{showGiftView}}"><view class="wrap__giftPanel data-v-6a66df3e"><view data-event-opts="{{[['tap',[['close',['$event']]]]]}}" class="wrapgift-mask data-v-6a66df3e" onTap="__e"></view><view data-event-opts="{{[['tap',[['',['$event']]]]]}}" class="pop__ui_child anim-footer data-v-6a66df3e" catchTap="__e"><scroll-view class="wrapgift-body data-v-6a66df3e"><view class="wrap_giftlist data-v-6a66df3e"><view class="gift-head data-v-6a66df3e"><view class="gift-menu data-v-6a66df3e"><text class="gm-item gm-item-on data-v-6a66df3e">礼物</text></view><view class="gift-coins data-v-6a66df3e"><text class="gc-coin data-v-6a66df3e">{{gift_name+"："}}</text><text class="gc-num data-v-6a66df3e">{{userInfo.gift_money}}</text><text data-event-opts="{{[['tap',[['PlanFunc',['$event']]]]]}}" class="gc-recharge data-v-6a66df3e" onTap="__e">充值</text></view></view><swiper class="gift-swiper data-v-6a66df3e" indicator-dots="{{true}}" duration="{{200}}" indicator-color="#dbdbdb" indicator-active-color="#999"><swiper-item class="data-v-6a66df3e" a:for="{{giftList}}" a:for-item="group" a:for-index="num" a:key="num"><view class="gift__cells data-v-6a66df3e"><block class="data-v-6a66df3e" a:for="{{group}}" a:for-item="item" a:for-index="index" a:key="index"><block a:if="{{item.gift_id!=isGift}}"><view data-event-opts="{{[['tap',[['chooseGift',['$0'],[[['giftList','',num],['','',index]]]]]]]}}" class="giftitem data-v-6a66df3e" onTap="__e"><view class="giftitem-rect data-v-6a66df3e"><image class="giftimg data-v-6a66df3e" src="{{item.file_path}}" mode="aspectFill"></image></view><text class="giftname data-v-6a66df3e">{{item.gift_name}}</text><text class="giftcoin data-v-6a66df3e">{{item.price+gift_name}}</text></view></block><block a:if="{{item.gift_id==isGift}}"><view class="giftitem data-v-6a66df3e"><view class="giftitem_active data-v-6a66df3e"><view class="giftitem-rect data-v-6a66df3e"><image class="giftimg_active data-v-6a66df3e" src="{{item.file_path}}" mode="aspectFill"></image></view><text class="giftcoin data-v-6a66df3e">{{item.price+gift_name}}</text><button data-event-opts="{{[['tap',[['sendGift',['$0'],[[['giftList','',num],['','',index]]]]]]]}}" class="sendbtn data-v-6a66df3e" onTap="__e"><text class="btntext data-v-6a66df3e">发送</text></button></view></view></block></block></view></swiper-item></swiper></view></scroll-view></view></view></block>