
.diy-notice {
	margin: 20rpx;
	display: flex;
	justify-content: flex-start;
	align-items: center;
	border-radius: 12rpx;
}
.diy-notice .notice-icon {
	width: 32rpx;
	height: 32rpx;
}
.diy-notice .notice-icon image {
	width: 100%;
	height: 100%;
}
.diy-notice .notice-text {
	overflow: hidden;
	white-space: nowrap;
	margin-left: 10px;
	font-size: 32rpx;
}

