<block a:if="{{!loadding}}"><view class="evaluate pb100"><form data-event-opts="{{[['submit',[['formSubmit',['$event']]]]]}}" onSubmit="__e" onReset="formReset"><view class="evaluate-item p30" a:for="{{tableData}}" a:for-item="item" a:for-index="index" a:key="index"><view class="product d-s-c"><view class="cover"><image src="{{item.image.file_path}}" mode="aspectFit"></image></view><view class="info ml20"><view class="name f28">{{item.product_name}}</view><view class="price pt10 f22 red">¥<text class="f40">{{item.product_price}}</text></view></view></view><view class="grade evalu p-30-0 mt30"><view class="evalu-value">描述相符：<view class="eval"><view data-event-opts="{{[['tap',[['choosees',[indexEv,index]]]]]}}" class="{{(('_i')+' '+(itemEv?'icon iconfont icon-start':'icon iconfont icon-start1'))}}" onTap="__e" a:for="{{describe[index]}}" a:for-item="itemEv" a:for-index="indexEv" a:key="indexEv"></view></view></view><view class="evalu-value">服务态度：<view class="eval"><view data-event-opts="{{[['tap',[['chooseServ',[indexEv,index]]]]]}}" class="{{(('_i')+' '+(itemEv?'icon iconfont icon-start':'icon iconfont icon-start1'))}}" onTap="__e" a:for="{{service[index]}}" a:for-item="itemEv" a:for-index="indexEv" a:key="indexEv"></view></view></view><view class="evalu-value">配送服务：<view class="eval"><view data-event-opts="{{[['tap',[['chooseLog',[indexEv,index]]]]]}}" class="{{(('_i')+' '+(itemEv?'icon iconfont icon-start':'icon iconfont icon-start1'))}}" onTap="__e" a:for="{{logistics[index]}}" a:for-item="itemEv" a:for-index="indexEv" a:key="indexEv"></view></view></view></view><view class="grade d-a-c p-30-0 mt30"><view data-event-opts="{{[['tap',[['gradeFunc',['$0',10,index],[[['tableData','',index]]]]]]]}}" class="{{(item.score==10?'flex-1 d-c-c active':'flex-1 d-c-c')}}" onTap="__e"><view class="item d-c-c"><text class="icon iconfont icon-pingjiahaoping"></text><text class="ml10">好评</text></view></view><view data-event-opts="{{[['tap',[['gradeFunc',['$0',20,index],[[['tableData','',index]]]]]]]}}" class="{{(item.score==20?'flex-1 d-c-c active':'flex-1 d-c-c')}}" onTap="__e"><view class="item d-c-c"><text class="icon iconfont icon-pingjiazhongping"></text><text class="ml10">中评</text></view></view><view data-event-opts="{{[['tap',[['gradeFunc',['$0',30,index],[[['tableData','',index]]]]]]]}}" class="{{(item.score==30?'flex-1 d-c-c active':'flex-1 d-c-c')}}" onTap="__e"><view class="item d-c-c"><text class="icon iconfont icon-pingjiachaping"></text><text class="ml10">差评</text></view></view></view><view class="textarea-box d-s-c f28"><textarea class="p10 box-s-b border flex-1" placeholder="请输入评价内容" data-event-opts="{{[['input',[['__set_model',['$0','content','$event',[]],[[['tableData','',index]]]]]]]}}" value="{{item.content}}" onInput="__e"></textarea></view><view value="{{item.image_list}}" data-event-opts="{{[['input',[['__set_model',['$0','image_list','$event',[]],[[['tableData','',index]]]]]]]}}" onInput="__e" class="upload-list d-s-c"><view data-event-opts="{{[['tap',[['deleteImg',[index,img_num]]]]]}}" class="item" onTap="__e" a:for="{{item.image_list}}" a:for-item="imgs" a:for-index="img_num" a:key="img_num"><image src="{{imgs.file_path}}" mode="aspectFit"></image></view><block a:if="{{item.image_list.length<9}}"><view data-event-opts="{{[['tap',[['openUpload',[index]]]]]}}" class="item upload-btn d-c-c d-c" onTap="__e"><text class="icon iconfont icon-xiangji"></text><text class="gray9">上传图片</text></view></block></view></view><view class="foot-btns"><button class="btn-red" form-type="submit">确认提交</button></view></form><block a:if="{{isUpload}}"><upload onGetImgs="__e" vue-id="9b868bd2-1" data-event-opts="{{[['^getImgs',[['getImgsFunc']]]]}}" onVueInit="__l"></upload></block></view></block>