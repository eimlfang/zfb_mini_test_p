
.address-list {
	padding-bottom: 90rpx;
}
.foot-btns {
	padding: 0;
}
.foot-btns .btn-red {
	width: 100%;
	height: 90rpx;
	line-height: 90rpx;
	border-radius: 0;
}
.address-distr_open{
	position: absolute;
	bottom: env(safe-area-inset-bottom);
	left: 0;
	width: 100%;
	display: block;
}
.address-distr_close{
	position: absolute;
	bottom: env(safe-area-inset-bottom);
	left: 0;
	width: 100%;
	display: none;
}

