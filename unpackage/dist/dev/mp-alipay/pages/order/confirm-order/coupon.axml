<view class="{{(Visible?'usable-coupon open':'usable-coupon close')}}"><view data-event-opts="{{[['tap',[['closePopup',['$event']]]]]}}" class="popup-bg" onTap="__e"></view><view data-event-opts="{{[['tap',[['',['$event']]]]]}}" class="main pt30" catchTap="__e"><scroll-view class="scroll-Y" style="{{('height:'+scrollviewHigh+'px;')}}" scroll-y="true"><view class="p-0-30"><view data-event-opts="{{[['tap',[['selectCoupon',['$0'],[[['datalist','',index,'user_coupon_id']]]]]]]}}" class="{{('coupon-item coupon-item-'+item.color.text)}}" style="margin-bottom:15rpx;" onTap="__e" a:for="{{datalist}}" a:for-item="item" a:for-index="index" a:key="index"><view class="circles"><text a:for="{{6}}" a:for-item="circle" a:for-index="num" a:key="num"></text></view><view class="info w100"><view class="d-c-c d-c"><text class="f40 fb w-s-n">{{item.coupon_type.text}}</text></view></view><view class="operation d-b-c w-b"><view class="flex-1 o-h"><view class="f34">{{item.name}}</view><view class=" f24">{{"最低消费"+item.min_price+"元"}}</view><block a:if="{{item.expire_type==10}}"><block><view class="mt30 f24 ">{{"领取"+item.expire_day+"天内有效"}}</view></block></block><block a:if="{{item.expire_type==20}}"><block><view class="mt30 f24 red">{{item.start_time.text+"~"+item.end_time.text}}</view></block></block></view><view class="f30 mr20 b-radio">立即使用</view></view></view></view></scroll-view><view class="coupon-btns"><button class="btn-cancel" type="default" data-event-opts="{{[['tap',[['closePopup',[0]]]]]}}" onTap="__e">不使用优惠券</button></view></view></view>