<view class="im_index"><view data-event-opts="{{[['tap',[['jumpPage',['$0','$1'],[[['imList','',index,'user_id']],[['imList','',index,'user.nickName']]]]]]]}}" class="im_item" onTap="__e" a:for="{{imList}}" a:for-item="item" a:for-index="index" a:key="index"><view class="im_item_left"><image src="{{item.user.avatarUrl}}" mode></image></view><view class="im_item_right"><view class="im_item_right_item"><view class="title">{{item.user.nickName}}</view><view class="time">{{item.newMessage.create_time}}</view></view><view class="im_item_right_item"><block a:if="{{item.newMessage.type==0}}"><view class="content">{{item.newMessage.content}}</view></block><block a:if="{{item.newMessage.type==1}}"><view class="content">[图片]</view></block><block a:if="{{item.newMessage.type==2}}"><view class="content">[商品]</view></block><block a:if="{{item.num>0}}"><view class="message">{{item.num}}</view></block></view></view></view><block a:if="{{imList.length==0}}"><view class="hint">您当前并没有与任何人聊天哦！</view></block></view>