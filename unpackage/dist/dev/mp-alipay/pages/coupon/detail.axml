<block a:if="{{!loading}}"><view class="coupon_bg"><view class="coupon-detail-top"><view class="top_box"><view><text class="icon iconfont icon-dianpu1" style="color:#333333;font-size:30rpx;"></text><text class="gray3 fb f30 ml10">{{detail.supplier?detail.supplier.name:'平台通用'}}</text></view><view class="item-wrap"><view class="{{('coupon-item coupon-item-'+detail.color.text)}}"><view class="circles"><text a:for="{{8}}" a:for-item="circle" a:for-index="num" a:key="num"></text></view><view class="info"><view>{{detail.coupon_type.text}}</view></view><view class="operation d-b-c w-b"><view class="flex-1 coupon-content"><view><block a:if="{{detail.coupon_type.value==10}}"><view class="price"><text>￥</text><text class="f40 fb">{{detail.reduce_price}}</text></view></block><block a:if="{{detail.coupon_type.value==20}}"><text class="f40 fb">{{detail.discount}}</text><text>折</text></block></view><view class="f30">{{detail.name}}</view><view class="f24"><block a:if="{{detail.expire_type==20}}">{{'有效期：'+detail.start_time.text+" 至 "+detail.end_time.text+''}}</block><block a:if="{{detail.expire_type==10}}">{{'有效期：领取后'+detail.expire_day+'天有效'}}</block></view></view><view data-event-opts="{{[['tap',[['receiveCoupon']]]]}}" class="btns d-c-c" onTap="__e"><block a:if="{{detail.state.value==1}}"><button class="{{('btn-red')}}" type="default">立即领取</button></block><block a:else><button class="btn-gray" type="default" data-event-opts="{{[['tap',[['',['$event']]]]]}}" catchTap="__e">{{''+detail.state.text+''}}</button></block></view></view></view></view></view><view class="redF6 f26 fb pt30 d-c-c"><image class="decorate" src="/static/icon/coupons_arrow.png" mode></image><text class="ml10 mr10">指定以下商品使用</text><image class="decorate" src="/static/icon/coupons_arrow.png" mode></image></view><block a:if="{{apply_range!=10}}"><view class="o-h pro_list"><view class="d-b-c f-w"><view data-event-opts="{{[['tap',[['gotoPage',['/pages/product/detail/detail?product_id='+item.product_id]]]]]}}" class="pro_item" onTap="__e" a:for="{{listData}}" a:for-item="item" a:for-index="index" a:key="index"><view class="pro_item_image"><image src="{{item.product_image||'/static/default.png'}}" mode="aspectFill"></image></view><view class="f26 gray3 text-ellipsis mt20 mb23 tc">{{item.product_name}}</view><view class="fb redF6 tc"><text class="f22">￥</text><text class="f32">{{item.product_price}}</text></view><view class="d-c-c"><button class="add_btn">加入购物车</button></view></view></view><block a:if="{{listData.length==0&&!loading}}"><view class="d-c-c p30 ww100"><text class="iconfont icon-wushuju"></text><text class="cont">亲，暂无相关记录哦</text></view></block><block a:else><uni-load-more vue-id="2543e56a-1" loadingType="{{loadingType}}" onVueInit="__l"></uni-load-more></block></view></block></view></view></block>