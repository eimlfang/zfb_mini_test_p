}
.theme6 .product-popup .price {
  color: #e31c28 !important;
}
.product-popup .price .num {
  padding: 0 4rpx;
  font-size: 40rpx;
}
.product-popup .old-price {
  margin-left: 10rpx;
  font-size: 26rpx;
  color: #999999;
  text-decoration: line-through;
}
.product-popup .body {
  padding: 20rpx 20rpx;
  max-height: 900rpx;
  overflow-y: auto;
}
.product-popup .level-box {
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.product-popup .level-box .key {
  font-size: 24rpx;
