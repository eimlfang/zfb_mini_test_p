.A_display {
  width: 100%;
  height: auto;
}
.active::after {
  content: '';
  width: 40%;
  height: 1px;
  display: block;
  margin: 0 auto;
  border-bottom: 1.5px solid #1CCFC9;
  padding: 1px;
  color: #1CCFC9;
}
.active_bg {
  background: #1CCFC9 !important;
}
.active_text {
  color: #1CCFC9 !important;
}
.A_display-bg {
  width: 100%;
  height: 119px;
  /* position: relative; */
  z-index: 999;
  background-image: url("https://img1.imgtp.com/2022/07/02/GGmxgTVf.png");
  background-repeat: no-repeat;
  background-size: 100% 170px;
}
.image1 {
  width: 100%;
  height: 170px;
}
.A_display-zhong {
  width: 100%;
  height: auto;
  border-radius: 10px;
  background: #fff;
  overflow: hidden;
  z-index: -999;
}
.zhong_top {
  width: 100%;
  height: 120px;
  border-bottom: 0.5px solid #EEEEEE;
  display: flex;
  position: relative;
  top: -44px;
}
.err {
  width: 25%;
  height: 119px;
  background-color: #fff;
  text-align: center;
  display: flex;
  flex-flow: column;
  align-items: center;
}
.err view {
  width: 100%;
  height: 50px;
  line-height: 50px;
}
.err image {
  width: 25px;
  height: 25px;
  position: relative;
  top: 26rpx;
  left: 25rpx;
}
.err text {
  font-size: 14px;
}
.err .bg-ccc {
  width: 50px;
  height: 50px;
  border-radius: 50%;
  background: #F8F8F8;
  margin-top: 15px;
}
.u-card__body {
  width: 100%;
  height: 1900rpx;
  padding: 15px 15px;
  z-index: -99;
}
.u-card__body .u-card__body__body {
  width: 100%;
  height: auto;
}
.u-card__body .u-card__body__body .title-big {
  width: 100%;
  height: 45px;
  line-height: 67px;
  font-size: 18px;
  font-weight: 900;
  padding-top: 10rpx;
}
.u-card__body .u-card__body__body .u-page__item {
  padding: 0 20rpx;
}
.u-card__body .u-card__body__body .title-small {
  width: 100%;
  height: 55px;
  line-height: 55px;
  font-size: 15px;
  color: #666666;
  border-bottom: 0.5px solid #EEEEEE;
  display: flex;
  justify-content: space-between;
  text-align: center;
}
.u-card__body .u-card__body__body .title-small image {
  margin-left: 15rpx;
}
.dibu {
  width: 100%;
  height: 50px;
  line-height: 70px;
  background-color: #F5F5F5;
  display: flex;
  align-items: center;
  justify-content: center;
}
.dibu .dibu1 {
  color: #999999;
}
.dibu .my-fans {
  color: #00C8BE;
}
.ding {
  height: 300rpx;
  background-color: aquamarine;
}
.st {
  height: 120px;
  width: 750rpx;
  background-color: #fff;
  z-index: 999;
}
.sticky-fixed {
  position: -webkit-sticky;
  position: sticky;
  top: 44px;
  position: fixed;
  top: 0;
  z-index: 999;
}
.xiala {
  height: 2500px;
  background-color: #eee;
  padding-top: 100rpx;
}
.questionbag {
  position: -webkit-sticky;
  position: sticky;
  top: 0px;
  z-index: 99;
  background: #fff;
}
.questionbag1 {
  background: #fff;
  position: fixed;
  top: 0;
  left: 0;
}
.cidy {
  position: relative;
  top: 0;
}

