.add {
  border-radius: 50rpx;
  padding: 10rpx 20rpx;
  text-align: center;
  height: 30rpx;
  line-height: 30rpx;
  color: #F2AE3F;
  border: 1rpx solid #F2AE3F;
}
.bclxr {
  color: white;
  background-color: #F2AE3F;
  border-radius: 20rpx;
  margin: 15rpx;
}
.bclxr2 {
  color: white;
  background-color: #DCDFE6;
  border-radius: 20rpx;
  margin: 15rpx;
}
page {
  background-color: #F7F6FB;
}
.content {
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 20rpx 25rpx;
  border-radius: 25rpx;
  margin: 20rpx;
  background-color: #FFFFFF;
}
.content .left view {
  margin: 10rpx;
}
.content .left .top {
  color: #2F3131;
  font-size: 34rpx;
  font-weight: 700;
}
.content .left .buttom {
  color: #B1B1B1;
  font-size: 24rpx;
}
