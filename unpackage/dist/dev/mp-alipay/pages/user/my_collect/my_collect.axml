<view><scroll-view class="scroll-Y" style="{{('height:'+scrollviewHigh+'px;')}}" scroll-y="true" lower-threshold="50" data-event-opts="{{[['scrollToLower',[['scrolltolowerFunc',['$event']]]]]}}" onScrollToLower="__e"><view class="shop_list_body"><view class="shop_list_body_item" a:for="{{shop_list}}" a:for-item="item" a:for-index="index" a:key="index"><view data-event-opts="{{[['tap',[['goto_shop',['$0'],[[['shop_list','',index,'shop_supplier_id']]]]]]]}}" class="shop_list_body_item_shop" onTap="__e"><view class="shop_list_body_item_shop_logo"><image src="{{item.logo}}"></image></view><view class="shop_list_body_item_shop_info"><view class="h1 title">{{item.store_name}}</view><view class="h3 brand">{{"主营品牌："+item.categoryName}}</view><view class="h3 sales">{{"销量"+item.product_sales+"件"}}</view></view><view class="shop_list_body_item_shop_others"><view class="h3 attention"><text class="red">{{item.fav_count}}</text>人关注</view><view class="h3 collect">店铺评分：<text class="red">{{item.score}}</text></view></view></view><block a:if="{{shop_list[index].productList.length>0}}"><view class="{{(shop_list[index].productList.length<3?'shop_list_body_item_product2':'shop_list_body_item_product')}}"><view data-event-opts="{{[['tap',[['goto_product',['$0'],[[['shop_list.'+index+'.productList','',index2,'product_id']]]]]]]}}" class="shop_list_body_item_product_item" onTap="__e" a:for="{{shop_list[index].productList}}" a:for-item="item" a:for-index="index2" a:key="index2"><image src="{{item.logo}}"></image><view class="shop_list_body_item_product_item_price"><view class="h4 red">¥<text class="h3">{{item.product_price>1000?item.product_price*1:item.product_price}}</text></view><view class="h6 huaxianjia">¥<text class="h5">{{item.line_price>1000?item.line_price*1:item.line_price}}</text></view></view></view></view></block></view></view><block a:if="{{shop_list.length==0&&!loading}}"><view class="d-c-c p30"><text class="iconfont icon-wushuju"></text><text class="cont">亲，暂无相关记录哦</text></view></block><block a:else><uni-load-more vue-id="ef727450-1" loadingType="{{loadingType}}" onVueInit="__l"></uni-load-more></block></scroll-view></view>