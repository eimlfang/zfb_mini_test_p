<view class="{{($root.m0)}}" data-theme="{{$root.m1}}"><view class="points-top theme-bg d-b-c"><view><text class="f26">积分总数：</text><text class="f34 fb">{{points}}</text><block a:if="{{is_trans_balance}}"><text data-event-opts="{{[['tap',[['e0',['$event']]]]]}}" class="f24 re-btn" onTap="__e">提现</text></block></view><block a:if="{{is_open}}"><button class="theme-borderbtn" type="default" data-event-opts="{{[['tap',[['gotoShop',['$event']]]]]}}" onTap="__e">积分商城</button></block></view><view class="p-0-30 bg-white"><view class="d-b-c border-b p-30-0" a:for="{{tableData}}" a:for-item="item" a:for-index="index" a:key="index"><view class="d-s-s f-w d-c flex-1"><text class="f24">{{item.describe}}</text><text class="pt10 gray9 f22">{{item.create_time}}</text></view><block a:if="{{item.value>0}}"><view class="red">{{"+"+item.value}}</view></block><block a:else><view class="red">{{item.value}}</view></block></view><block a:if="{{tableData.length==0&&!loading}}"><view class="d-c-c p30"><text class="iconfont icon-wushuju"></text><text class="cont">亲，暂无相关记录哦</text></view></block><block a:else><uni-load-more vue-id="12b35a58-1" loadingType="{{loadingType}}" onVueInit="__l"></uni-load-more></block></view><recharge vue-id="12b35a58-2" isPop="{{isPop}}" discount_ratio="{{discount_ratio}}" data-event-opts="{{[['^close',[['closePop']]]]}}" onClose="__e" onVueInit="__l"></recharge></view>