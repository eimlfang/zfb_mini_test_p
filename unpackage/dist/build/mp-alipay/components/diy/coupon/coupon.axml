<view class="diy-coupon" style="{{'background:'+(itemData.style.background)+';'+('padding:'+(itemData.style.paddingTop+'px 20rpx')+';')}}"><scroll-view scroll-x="true"><view class="swiper swiper-list"><view class="{{((('coupon-item')+' '+'d-b-c')+' '+('bg-'+item.color.text))}}" a:for="{{listData}}" a:for-item="item" a:for-index="index" a:key="index"><view class="side-line left-side-line"><text class="round" style="{{'background:'+(itemData.style.background)+';'}}"></text></view><view class="side-line right-side-line"><text class="round" style="{{'background:'+(itemData.style.background)+';'}}"></text></view><view class="center-content pr"><view class="content-top"><block a:if="{{item.coupon_type.value==10}}"><text class="f24">￥</text><text class="f40">{{item.reduce_price*1}}</text></block><block a:if="{{item.coupon_type.value==20}}"><text class="f40">{{item.discount}}</text><text class="f24">折</text></block></view><view class="content-bottom tc"><text class="f20">{{item.name}}</text></view></view><block a:if="{{item.state.value==1}}"><view data-event-opts="{{[['tap',[['receiveCoupon',[index]]]]]}}" class="right-receive" onTap="__e"><view class="lr-tb">立即领取</view></view></block><block a:else><view class="right-receive no-receive"><view class="lr-tb">{{item.state.text}}</view></view></block></view></view></scroll-view></view>