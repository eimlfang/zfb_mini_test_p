<view class="diy-article"><view data-event-opts="{{[['tap',[['gotoPageFunc',['$0'],[[['listData','',index]]]]]]]}}" class="{{(('article-item')+' '+('show-type__'+itemData.style.display))}}" onTap="__e" a:for="{{listData}}" a:for-item="item" a:for-index="index" a:key="index"><block a:if="{{itemData.style.display==10}}"><block a:if="{{item.image!=null&&item.image.file_path!=''}}"><view class="picture mr15"><image src="{{item.image.file_path}}" mode="aspectFill"></image></view></block><view class="d-b-c d-stretch d-c flex-1"><view class="text-ellipsis-2 f24">{{''+item.article_title+''}}</view><view class="d-b-c gray9 f18"><view><text class="icon iconfont icon-chakan" style="font-size:22rpx;"></text><text class="ml10">{{item.actual_views}}</text></view><view><text class="icon iconfont icon-daojishi mr10"></text>{{item.create_time}}</view></view></view></block><block a:if="{{itemData.style.display==20}}"><view class="f24 text-ellipsis-2 lh200">{{''+item.article_title+''}}</view></block></view></view>