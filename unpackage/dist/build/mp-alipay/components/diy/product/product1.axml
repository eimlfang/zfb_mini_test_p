<view class="diy-product"><view class="display__slide"><scroll-view scroll-top="{{scrollTop}}" scroll-x="true"><view class="{{(('product-list')+' '+'column__2')}}"><view data-event-opts="{{[['tap',[['gotoDetail',['$0'],[[['itemData.data','',index,'product_id']]]]]]]}}" style="position:relative;" onTap="__e" a:for="{{itemData.data}}" a:for-item="product" a:for-index="index" a:key="index"><view class="product-item"><view class="product-cover"><image src="{{product.product_image}}" mode="aspectFill"></image><view class="rexiao">{{'TOP'+(index+1)+''}}</view></view><view class="product-info"><view class="product-title">{{''+product.product_name}}</view><view class="price d-s-c f12"><view style="color:#FF6975;"><text>¥</text><text>{{product.product_price+"/天"}}</text></view></view></view></view></view></view></scroll-view></view></view>