<block a:if="{{listData.length>0}}"><view class="diy-special"><view class="special-left"><image src="{{itemData.style.image}}" mode="aspectFill"></image></view><view class="right-shadow"></view><view class="{{(('special-content')+' '+('display_'+itemData.style.display))}}"><view class="special-content-list" style="{{('transform: translate(0,'+styleValue+'px);')}}"><view data-event-opts="{{[['tap',[['gotoPageFunc',['$0'],[[['listData','',index]]]]]]]}}" class="content-item text-ellipsis" onTap="__e" a:for="{{listData}}" a:for-item="item" a:for-index="index" a:key="index"><text>{{item.article_title}}</text></view></view></view><view data-event-opts="{{[['tap',[['gotoPageFunc']]]]}}" class="special-more" onTap="__e"><text class="iconfont icon-jiantou"></text></view></view></block>