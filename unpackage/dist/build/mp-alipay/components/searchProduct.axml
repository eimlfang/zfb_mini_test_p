<block a:if="{{isShow}}"><view data-event-opts="{{[['touchMove',[['stopTouchMove',['$event']]]]]}}" catchTouchMove="__e"><view class="search-wrap"><view class="index-search-box d-b-c" style="{{($root.m0==0?'':'height:'+$root.m1+'px;padding-right: 200rpx')}}" id="searchBox"><view class="reg180" style="{{($root.m2==0?'':'height:'+$root.m3+'px;')}}"><text data-event-opts="{{[['tap',[['closeSearch',['$event']]]]]}}" class="icon iconfont icon-jiantou" onTap="__e"></text></view><view class="index-search t-c flex-1" style="{{($root.m4==0?'':'height:'+$root.m5+'px;')}}"><text class="icon iconfont icon-sousuo"></text><input class="flex-1 ml10 f30 gray3" type="text" value="{{form.keyWord}}" placeholder-class="f24 gray6" placeholder="输入你要的商品" confirm-type="search" data-event-opts="{{[['confirm',[['search']]],['input',[['__set_model',['$0','keyWord','$event',[]],['form']]]]]}}" onConfirm="__e" onInput="__e"/></view></view><view class="p-0-20 bg-white"><view class="group-hd"><view class="left"><text class="min-name">最近搜索</text></view><view class="right"><text data-event-opts="{{[['tap',[['clearStorage',['$event']]]]]}}" class="icon iconfont icon-lajitong" onTap="__e"></text></view></view><view class="before-search"><text data-event-opts="{{[['tap',[['search',['$0'],['arr.'+index+'']]]]]}}" class="item" onTap="__e" a:for="{{arr}}" a:for-item="item" a:for-index="index" a:key="index">{{arr[index]}}</text></view></view></view></view></block>