<block a:if="{{!loadding}}"><view><block a:if="{{listData.length>0}}"><view><view class="address-list bg-white"><view data-event-opts="{{[['tap',[['addAddress']]]]}}" class="tr add_add" onTap="__e">新增收货地址</view><view class="address p-30-0 d-s-c border-b-d" a:for="{{listData}}" a:for-item="item" a:for-index="index" a:key="index"><view class="radio"><radio style="transform:scale(0.8);" color="#F6220C" value="{{item.address_id+''}}" checked="{{default_id==item.address_id+''}}" data-event-opts="{{[['tap',[['radioChange',['$0'],[[['listData','',index,'address_id']]]]]]]}}" onTap="__e"></radio></view><view class="info flex-1 ml20"><view class="user f32"><text>{{item.name}}</text><text class="ml20 gray9 f26">{{item.phone}}</text><block a:if="{{default_id==item.address_id}}"><text class="ml20 defaul_add">默认</text></block></view><view class="pt20 f26 gray3">{{''+item.region.province+item.region.city+item.region.region+item.detail+''}}</view></view><view data-event-opts="{{[['tap',[['delAddress',['$0'],[[['listData','',index,'address_id']]]]]]]}}" class="icon-box plus d-c-c ml30" onTap="__e"><image class="add_icon_img" src="../../../static/icon/delete.png" mode="widthFix"></image></view><view class="none_line ml30"></view><view data-event-opts="{{[['tap',[['editAddress',['$0'],[[['listData','',index,'address_id']]]]]]]}}" class="icon-box plus d-c-c ml30 mr40" onTap="__e"><image class="add_icon_img" src="../../../static/icon/edit.png" mode="widthFix"></image></view></view></view></view></block><block a:else><view><view class="none_add"><image class="no_add" src="../../../static/no_adress.png" mode></image></view><view data-event-opts="{{[['tap',[['addAddress']]]]}}" class="no_add_add" onTap="__e">新增收货地址</view></view></block></view></block>