<block a:if="{{!loadding}}"><view class="refund-apply pb100"><form data-event-opts="{{[['submit',[['formSubmit',['$event']]]]]}}" onSubmit="__e" onReset="formReset"><view class="one-product d-s-c p30 bg-white "><view class="cover"><image src="{{product.image.file_path||''}}" mode="aspectFit"></image></view><view class="flex-1"><view class="pro-info">{{product.product_name}}</view><view class="pt10 p-0-30 f24 gray9"><text>{{'单价：¥'+product.line_price+''}}</text><text class="ml20">{{'数量：'+product.total_num+''}}</text></view></view></view><view class="group bg-white"><view class="group-hd border-b-e"><view class="left"><text class="min-name">服务类型</text></view></view><view class="d-s-c p-20-0"><block a:if="{{product.orderM.delivery_type.value!=30}}"><button class="{{(type==10?'btn-red-border':'')}}" type="default" data-event-opts="{{[['tap',[['tabType',[10]]]]]}}" onTap="__e">退货/退款</button></block><block a:if="{{product.orderM.delivery_type.value!=30}}"><button class="{{(type==20?'ml20 btn-red-border':'ml20')}}" type="default" data-event-opts="{{[['tap',[['tabType',[20]]]]]}}" onTap="__e">换货</button></block><block a:if="{{product.orderM.delivery_type.value!=30}}"><button class="{{(type==30?'ml20 btn-red-border':'ml20')}}" type="default" data-event-opts="{{[['tap',[['tabType',[30]]]]]}}" onTap="__e">仅退款</button></block></view></view><view class="group bg-white"><view class="group-hd"><view class="left"><text class="min-name">申请原因</text></view></view><view class="d-s-c"><textarea class="p10 box-s-b border flex-1 f28 lh150" value="" name="content" placeholder="请详细填写申请原因，注意保持商品的完好，建议您先与卖家沟通"></textarea></view></view><block a:if="{{type==10||type==30}}"><view class="group bg-white"><view class="group-hd"><view class="left"><text class="min-name">退款金额：</text><text class="red f30">{{"¥"+product.total_price}}</text></view></view></view></block><view class="group bg-white"><view class="group-hd"><view class="left"><text class="min-name">上传凭证</text><text class="gray9">(最多6张)</text></view></view><view class="upload-list d-s-c"><view data-event-opts="{{[['tap',[['deleteFunc',['$0'],[[['images','',img_num]]]]]]]}}" class="item" onTap="__e" a:for="{{images}}" a:for-item="imgs" a:for-index="img_num" a:key="img_num"><image src="{{imgs.file_path}}" mode="aspectFit"></image></view><block a:if="{{images.length<6}}"><view class="item d-c-c d-stretch"><view data-event-opts="{{[['tap',[['openUpload']]]]}}" class="upload-btn d-c-c d-c flex-1" onTap="__e"><text class="icon iconfont icon-xiangji f34"></text><text class="gray9">上传图片</text></view></view></block></view></view><view class="foot-btns"><button class="btn-red" form-type="submit">确认提交</button></view></form><block a:if="{{isUpload}}"><upload onGetImgs="__e" vue-id="d5c165e8-1" data-event-opts="{{[['^getImgs',[['getImgsFunc']]]]}}" onVueInit="__l"></upload></block></view></block>