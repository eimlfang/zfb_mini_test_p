<view class="look-evaluate"><view class="top-tabbar"><view data-event-opts="{{[['tap',[['stateFunc',[0]]]]]}}" class="{{(state_active==-1?'tab-item active':'tab-item')}}" onTap="__e">{{"全部("+Total.all+')'}}</view><view data-event-opts="{{[['tap',[['stateFunc',[10]]]]]}}" class="{{(state_active==10?'tab-item active':'tab-item')}}" onTap="__e"><view class="d-c-c"><text class="icon iconfont icon-pingjiahaoping"></text><text class="ml10 gray9">{{"好评("+Total.praise+")"}}</text></view></view><view data-event-opts="{{[['tap',[['stateFunc',[20]]]]]}}" class="{{(state_active==20?'tab-item active':'tab-item')}}" onTap="__e"><view class="d-c-c"><text class="icon iconfont icon-pingjiazhongping"></text><text class="ml10 gray9">{{"中评("+Total.review+")"}}</text></view></view><view data-event-opts="{{[['tap',[['stateFunc',[30]]]]]}}" class="{{(state_active==30?'tab-item active':'tab-item')}}" onTap="__e"><view class="d-c-c"><text class="icon iconfont icon-pingjiachaping"></text><text class="ml10 gray9">{{"差评("+Total.negative+")"}}</text></view></view></view><scroll-view class="scroll-Y" style="{{('height:'+scrollviewHigh+'px;')}}" scroll-y="true" lower-threshold="50" data-event-opts="{{[['scrollToLower',[['scrolltolowerFunc',['$event']]]]]}}" onScrollToLower="__e"><view class="comment-list"><view class="item" a:for="{{tableData}}" a:for-item="item" a:for-index="index" a:key="index"><view class="cmt-user"><view class="left"><image class="photo" src="{{item.users.avatarUrl}}" mode="aspectFill"></image><text class="name">{{item.users.nickName}}</text></view></view><view class="d-b-c p20"><view class="d-s-c"><block a:if="{{item.score==10}}"><view class="d-c-c mr20"><text class="icon iconfont icon-pingjiahaoping"></text><text class="ml10 gray9">好评</text></view></block><block a:if="{{item.score==20}}"><view class="d-c-c mr20"><text class="icon iconfont icon-pingjiazhongping"></text><text class="ml10 gray9">中评</text></view></block><block a:if="{{item.score==30}}"><view class="d-c-c mr20"><text class="icon iconfont icon-pingjiachaping"></text><text class="ml10 gray9">差评</text></view></block></view><text class="datetime gray9">{{item.create_time}}</text></view><view class="p-0-20 f24 gray3">{{item.content}}</view><view class="imgs d-s-c p-0-20"><view class="box" a:for="{{item.image}}" a:for-item="imgs" a:for-index="img_num" a:key="img_num"><image src="{{imgs.file_path}}" mode="aspectFill" width data-event-opts="{{[['tap',[['preview',['$0','$1'],[[['tableData','',index,'image']],[['tableData','',index],['image','',img_num,'file_path']]]]]]]}}" onTap="__e"></image></view></view></view><block a:if="{{tableData.length==0&&!loading}}"><view class="d-c-c p30"><view class="none-data-box"><image src="/static/none.png" mode="widthFix"></image><text>亲，暂无相关记录哦</text></view></view></block><block a:else><uni-load-more vue-id="9b9b89fc-1" loadingType="{{loadingType}}" onVueInit="__l"></uni-load-more></block></view></scroll-view><block a:if="{{isopenimg}}"><view data-event-opts="{{[['tap',[['e0',['$event']]]]]}}" class="popimg" onTap="__e"><image src="{{popImg}}" mode="aspectFit"></image></view></block></view>