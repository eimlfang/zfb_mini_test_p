<block a:if="{{!loadding}}"><view class="index-agent o-h"><view class="banner d-c-c d-c"><image src="{{top_background}}" mode="widthFix"></image></view><block a:if="{{is_agent&&isData}}"><view class="agent-wrap pr m-0-20" style="margin-top:300rpx;"><view class="d-b-c border-b pb30 f28 lh150"><view><text class="gray3 f32">用户名：</text><text class="gray3 f32">{{user.nickName}}</text></view><view><text class="gray3 f32">{{info_words.index.words.referee.value+"："}}</text><text class="f32 gray3">{{agent.referee?agent.referee.nickName:'平台'}}</text></view></view><view class="d-s-c p-30-0 top_dash"><view class="flex-1 d-c-c d-c"><view class="redF6"><text class="f24">￥</text><text class="f40">{{agent.money}}</text></view><view class="pt20 f26 gray3">{{info_words.index.words.money.value}}</view></view><view class="flex-1 d-c-c d-c"><view><text class="f24">￥</text><text class="f40">{{agent.freeze_money}}</text></view><view class="pt20 f28 gray3">{{info_words.index.words.freeze_money.value}}</view></view><view class="flex-1 d-c-c d-c"><view><text class="f24">￥</text><text class="f40">{{agent.total_money}}</text></view><view class="pt20 f28 gray3">{{info_words.index.words.total_money.value}}</view></view></view><view class="d-c-c pt30"><button class="btn-gcred flex-1" type="primary" data-event-opts="{{[['tap',[['gotoCash',['$event']]]]]}}" onTap="__e">{{info_words.index.words.cash.value}}</button></view></view><view class="agent-wrap m-0-20 p30 d-s-c f-w mt20 bg-white"><view data-event-opts="{{[['tap',[['gotoPage',['/pages/agent/cash/list/list']]]]]}}" class="d-c-c d-c flex-1" onTap="__e"><view><image class="agent_index_img" src="../../../static/icon/icon-zijinmingxi.png" mode></image></view><text class="pt10 f26 mt20">{{info_words.cash_list.title.value}}</text></view><view data-event-opts="{{[['tap',[['gotoPage',['/pages/agent/order/order']]]]]}}" class="d-c-c d-c flex-1" onTap="__e"><view><image class="agent_index_img" src="../../../static/icon/icon-fenxiaodingdan.png" mode></image></view><text class="pt10 f26 mt20">{{info_words.order.title.value}}</text></view><view data-event-opts="{{[['tap',[['gotoPage',['/pages/agent/team/team']]]]]}}" class="d-c-c d-c flex-1" onTap="__e"><view><image class="agent_index_img" src="../../../static/icon/icon-tuandui.png" mode></image></view><text class="pt10 f26 mt20">{{info_words.team.title.value}}</text></view><view data-event-opts="{{[['tap',[['gotoPage',['/pages/agent/qrcode/qrcode']]]]]}}" class="d-c-c d-c flex-1" onTap="__e"><view><image class="agent_index_img" src="../../../static/icon/icon-erweima.png" mode></image></view><text class="pt10 f26 mt20">{{info_words.qrcode.title.value}}</text></view></view></block><block a:if="{{!is_agent&&isData}}"><view class="no-agent"><view class="mt50 p-0-20 red f34 tc">{{info_words.index.words.not_agent.value}}</view><view class="p30 mt30"><button class="btn-gcred" type="primary" data-event-opts="{{[['tap',[['applyagent',['$event']]]]]}}" onTap="__e">{{info_words.index.words.apply_now.value}}</button></view></view></block></view></block>