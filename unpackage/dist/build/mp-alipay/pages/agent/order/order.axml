<view><view class="top-tabbar"><view data-event-opts="{{[['tap',[['stateFunc',['$0'],[[['tableList','',index,'value']]]]]]]}}" class="{{(state_active==item.value?'tab-item active':'tab-item')}}" onTap="__e" a:for="{{tableList}}" a:for-item="item" a:for-index="index" a:key="index">{{item.text}}</view></view><scroll-view class="scroll-Y" style="{{('height:'+scrollviewHigh+'px;')}}" scroll-y="true" lower-threshold="50" data-event-opts="{{[['scrollToLower',[['scrolltolowerFunc',['$event']]]]]}}" onScrollToLower="__e"><view class="p-0-30 bg-white"><view class="border-b p-20-0" a:for="{{tableData}}" a:for-item="item" a:for-index="index" a:key="index"><view class="d-b-c f24"><text>{{"订单号"+item.order_master.order_no}}</text><block a:if="{{item.is_settled==1}}"><text class="blue">已结算</text></block><block a:else><text class="gray">未结算</text></block></view><view class="d-b-c mt20"><view class="agent-order-photo"><image src="{{item.user.avatarUrl}}" mode="aspectFill"></image></view><view class="flex-1 ml20 f24"><view class="d-b-c"><text class="gray3">{{item.user.nickName}}</text><block a:if="{{item.first_user_id==user_id}}"><text class="red">{{"+￥"+item.first_money}}</text></block><block a:if="{{item.second_user_id==user_id}}"><text class="red">{{"+￥"+item.second_money}}</text></block><block a:if="{{item.third_user_id==user_id}}"><text class="red">{{"+￥"+item.third_money}}</text></block></view><view class="d-b-c"><text class="gray9">{{"消费金额：￥"+item.order_price}}</text><text class="gray9">{{item.create_time}}</text></view></view></view></view><block a:if="{{tableData.length==0&&!loading}}"><view class="d-c-c p30"><text class="iconfont icon-wushuju"></text><text class="cont">亲，暂无相关记录哦</text></view></block><block a:else><uni-load-more vue-id="0841e782-1" loadingType="{{loadingType}}" onVueInit="__l"></uni-load-more></block></view></scroll-view></view>