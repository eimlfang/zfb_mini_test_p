<view class="bargain-container"><view class="top-box"><view class="inner-tab"><scroll-view class="scroll-X" scroll-X="true" show-scrollbar="false"><view class="tab-list"><view data-event-opts="{{[['tap',[['tabTypeFunc',[index,'$0'],[[['categorys','',index,'bargain_activity_id']]]]]]]}}" class="{{(bargain_activity_id==item.bargain_activity_id?'item active':'item')}}" onTap="__e" a:for="{{categorys}}" a:for-item="item" a:for-index="index" a:key="index">{{''+item.title+''}}</view></view></scroll-view></view><block a:if="{{listData.detail}}"><view class="banner-image d-c-c"><image src="{{listData.detail.file_path}}" mode="widthFix"></image></view></block><block a:if="{{listData.detail}}"><view class="ad-datetime p-30-0 d-c-c pb0"><countdown vue-id="7ddca034-1" config="{{countdownConfig}}" ref="__r" data-ref="countdown" data-event-opts="{{[['^returnVal',[['returnValFunc']]]]}}" onReturnVal="__e" onVueInit="__l"></countdown></view></block></view><block a:if="{{!loading}}"><view class="bargain-list"><scroll-view class="scroll-Y" style="{{('height:'+scrollviewHigh+'px;')}}" scroll-y="true" lower-threshold="50"><view class="list d-s-c f-w"><view class="item d-s-s d-stretch" a:for="{{listData.list}}" a:for-item="item" a:for-index="index" a:key="index"><view class="product-cover pr"><image src="{{item.product.file_path}}" mode="aspectFit"></image></view><view class="product-info d-b-c d-c"><view class="product-title text-ellipsis-2 f26 gray3">{{item.product.product_name}}</view><view class="people-num price"><text class="f24">￥</text><text class="f36 fb">{{item.bargain_price}}</text><text class="ml10 text-d-line gray9 f24">{{"￥"+item.product_price}}</text></view><view class="d-b-c ww100"><view class="f24"></view><view class="right-btn"><button type="primary" data-event-opts="{{[['tap',[['gotoDetail',['$0'],[[['listData.list','',index]]]]]]]}}" onTap="__e">去砍价</button></view></view></view></view></view></scroll-view></view></block></view>