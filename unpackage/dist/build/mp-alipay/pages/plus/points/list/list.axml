<view class="prodcut-list-wrap"><view class="point_top"><image src="../../../../static/point_bg.png" mode></image><view class="tc pr"><view class="f60 fb white">{{my_points}}</view><view class="f26 white">我的积分</view></view></view><view class="p-0-20 bg-white f32 gray3 fb ponit_title">好物兑换</view><view class="list"><view class="item" a:for="{{listData}}" a:for-item="item" a:for-index="index" a:key="index"><view class="product-cover"><image src="{{item.product_image}}" mode="aspectFit"></image></view><view class="product-info flex-1"><view class="product-title">{{item.product.product_name}}</view><view class="already-sale d-b-c"><text>{{"还剩"+item.stock+"件"}}</text></view><view class="price"><block a:if="{{item.sku[0].point_money!=0}}">¥<text class="num">{{item.sku[0].point_money}}</text><text class="f30">+</text></block><text class="num">{{item.sku[0].point_num}}</text><text class="f26">积分</text></view></view><view class="d-c-e"><view data-event-opts="{{[['tap',[['gotoList',['$0'],[[['listData','',index,'point_product_id']]]]]]]}}" class="point_btn" onTap="__e">兑换</view></view></view></view><block a:if="{{listData.length==0&&!loading}}"><view class="d-c-c p30"><text class="iconfont icon-wushuju"></text><text class="cont">亲，暂无相关记录哦</text></view></block><block a:else><uni-load-more vue-id="03cfa766-1" loadingType="{{loadingType}}" onVueInit="__l"></uni-load-more></block></view>